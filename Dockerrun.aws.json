{
  "AWSEBDockerrunVersion": 2,
  "Authentication": {
    "Bucket": "docker-registry-auth-amberhoule",
    "Key": ".dockercfg"
  },
  "containerDefinitions": [
    {
      "name": "server",
      "image": "amberhoule/secret-santa-server:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 4567,
          "containerPort": 4567
        }
      ]
    },
    {
      "name": "client",
      "image": "amberhoule/secret-santa-client:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ]
    }
  ]
}
